    "def convertEdge(M):\n",
    "    \n",
    "    graph = []\n",
    "    \n",
    "    w = len(M)\n",
    "    h = len(M[0])\n",
    "    node = 0\n",
    "    for i in range(w):\n",
    "        for j in range(h):\n",
    "            y, x, t = M[i][j]\n",
    "            graph.append([node,x, y, t])\n",
    "            node += 1\n",
    "    \n",
    "    return np.array(graph)"
   ]
  }
